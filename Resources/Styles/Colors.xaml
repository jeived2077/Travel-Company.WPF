<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!-- Dark Shades -->
    <Color x:Key="Primary1">#063251</Color>
    <Color x:Key="Primary2">#104e7a</Color>
    <Color x:Key="Primary3">#1a6aa2</Color>
    <Color x:Key="Primary4">#2d87c8</Color>
    <!-- Main Colour -->
    <Color x:Key="Primary5">#49a6e9</Color>
    <!-- Light Shades -->
    <Color x:Key="Primary6">#6ebef7</Color>
    <Color x:Key="Primary7">#8bcbf9</Color>
    <Color x:Key="Primary8">#a5d5f8</Color>
    <Color x:Key="Primary9">#ddebf5</Color>
    <Color x:Key="Primary10">#ebf7ff</Color>
    
    <!-- Headings -->
    <Color x:Key="Grey1">#102a42</Color>
    <Color x:Key="Grey2">#243a52</Color>
    <Color x:Key="Grey3">#324d67</Color>
    <Color x:Key="Grey4">#48647f</Color>
    <!-- Paragraphs -->
    <Color x:Key="Grey5">#617d98</Color>
    <Color x:Key="Grey6">#829ab0</Color>
    <Color x:Key="Grey7">#9eb2c7</Color>
    <Color x:Key="Grey8">#bcccdc</Color>
    <Color x:Key="Grey9">#d4eeff</Color>
    <Color x:Key="Grey10">#f1f5f8</Color>

    <!-- Dark Shades -->
    <SolidColorBrush x:Key="Primary1Brush"
                     Color="{StaticResource Primary1}" />
    <SolidColorBrush x:Key="Primary2Brush"
                     Color="{StaticResource Primary2}" />
    <SolidColorBrush x:Key="Primary3Brush"
                     Color="{StaticResource Primary3}" />
    <SolidColorBrush x:Key="Primary4Brush"
                     Color="{StaticResource Primary4}" />
    <!-- Main Colour -->
    <SolidColorBrush x:Key="Primary5Brush"
                     Color="{StaticResource Primary5}" />
    <!-- Light Shades -->
    <SolidColorBrush x:Key="Primary6Brush"
                     Color="{StaticResource Primary6}" />
    <SolidColorBrush x:Key="Primary7Brush"
                     Color="{StaticResource Primary7}" />
    <SolidColorBrush x:Key="Primary8Brush"
                     Color="{StaticResource Primary8}" />
    <SolidColorBrush x:Key="Primary9Brush"
                     Color="{StaticResource Primary9}" />
    <SolidColorBrush x:Key="Primary10Brush"
                     Color="{StaticResource Primary10}" />
    
    <!-- Headings -->
    <SolidColorBrush x:Key="Grey1Brush"
                     Color="{StaticResource Grey1}" />
    <SolidColorBrush x:Key="Grey2Brush"
                     Color="{StaticResource Grey2}" />
    <SolidColorBrush x:Key="Grey3Brush"
                     Color="{StaticResource Grey3}" />
    <SolidColorBrush x:Key="Grey4Brush"
                     Color="{StaticResource Grey4}" />
    <!-- Paragraphs -->
    <SolidColorBrush x:Key="Grey5Brush"
                     Color="{StaticResource Grey5}" />
    <SolidColorBrush x:Key="Grey6Brush"
                     Color="{StaticResource Grey6}" />
    <SolidColorBrush x:Key="Grey7Brush"
                     Color="{StaticResource Grey7}" />
    <SolidColorBrush x:Key="Grey8Brush"
                     Color="{StaticResource Grey8}" />
    <SolidColorBrush x:Key="Grey9Brush"
                     Color="{StaticResource Grey9}" />
    <SolidColorBrush x:Key="Grey10Brush"
                     Color="{StaticResource Grey10}" />

    <!-- Colors Resources -->
    <Color x:Key="White">#FFFFFF</Color>
    <Color x:Key="Black">#222222</Color>
    <Color x:Key="RedDark">#BB2525</Color>
    <Color x:Key="RedLight">#E66B6B</Color>
    <Color x:Key="GreenDark">#25BB32</Color>
    <Color x:Key="GreenLight">#6BE675</Color>

    <SolidColorBrush x:Key="WhiteBrush"
                     Color="{StaticResource White}" />
    <SolidColorBrush x:Key="BlackColorBrush"
                     Color="{StaticResource Black}" />
    <SolidColorBrush x:Key="RedDarkBrush"
                     Color="{StaticResource RedDark}" />
    <SolidColorBrush x:Key="RedLightBrush"
                     Color="{StaticResource RedLight}" />
    <SolidColorBrush x:Key="GreenDarkBrush"
                     Color="{StaticResource GreenDark}" />
    <SolidColorBrush x:Key="GreenLightBrush"
                     Color="{StaticResource GreenLight}" />
</ResourceDictionary>