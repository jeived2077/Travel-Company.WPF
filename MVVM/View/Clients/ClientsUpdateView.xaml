<UserControl x:Class="Travel_Company.WPF.MVVM.View.Clients.ClientsUpdateView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Travel_Company.WPF.MVVM.View.Clients"
             xmlns:component="clr-namespace:Travel_Company.WPF.Resources.Components"
             xmlns:clients="clr-namespace:Travel_Company.WPF.MVVM.ViewModel.Clients"
             d:DataContext="{d:DesignInstance Type=clients:ClientsUpdateViewModel}"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <component:ClientForm State="Updating"
                              RelayCommand="{Binding UpdateCommand}" />
    </Grid>
</UserControl>
