<UserControl x:Class="Travel_Company.WPF.MVVM.View.Employees.EmployeesCreateView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Travel_Company.WPF.MVVM.View.Employees"
             xmlns:component="clr-namespace:Travel_Company.WPF.Resources.Components"
             xmlns:employees="clr-namespace:Travel_Company.WPF.MVVM.ViewModel.Employees"
             d:DataContext="{d:DesignInstance Type=employees:EmployeesCreateViewModel}"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <component:EmployeeForm State="Inserting"
                                RelayCommand="{Binding CreateEmployeeCommand}" />
    </Grid>
</UserControl>
